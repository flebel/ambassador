{
  "Ambassadors": [
      {
          "Ambassador": "generic",
          "Path": "/jsonip1",
          "HTTPVerb": "GET",
          "Configuration": {
              "URL": "http://jsonip.com",
              "HTTPVerb": "GET"
          }
      },
      {
          "Ambassador": "jsonip",
          "Path": "/jsonip2",
          "HTTPVerb": "GET"
      },
      {
          "Ambassador": "ping",
          "Path": "/ping",
          "HTTPVerb": "GET"
      },
      {
          "Ambassador": "pushover",
          "Path": "/pushover",
          "HTTPVerb": "GET",
          "Configuration": {
            "Message": "This is a test message.",
            "Token": "PUSHOVER_APPLICATION_TOKEN_ID",
            "User": "PUSHOVER_USER_ID"
          }

      }

  ],
  "Listen": "localhost:8000"
}
